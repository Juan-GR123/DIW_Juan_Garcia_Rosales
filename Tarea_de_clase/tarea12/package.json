{
  "name": "tarea12",
  "version": "1.0.0",
  "description": "compilador de sass",
  "browserslist": ">0.25%, last 6 versions, not dead",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "compilar-sass": "sass --watch fuente/sass:fuente/styles",
    "desarrollo": "parcel index.html --dist-dir desarrollo --no-cache",
    "produccion": "parcel build index.html --dist-dir produccion --no-cache --public-url ../",
    "limpia": "rimraf desarrollo produccion dist .parcel-cache",
    "prod": "run-s limpia produccion",
    "des": "run-s limpia desarrollo",
    "todos": "run-s limpia produccion desarrollo",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "sass"
  ],
  "author": "Juan",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.13.3",
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.13.3",
    "parcel-plugin-imagemin": "^4.0.2",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "sass": "^1.83.4",
    "vite": "^6.2.0",
    "vite-plugin-html": "^3.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2"
  }
}
